%!
/str (00) def
1 1 100 { /i exch def
    /p 1 def
    i 3 mod 0 eq {                               % if divisible by 3
      /p 0 def                                     % clear flag
      (Fizz) print                                     % and print "Fizz"
    } if
    i 5 mod 0 eq {                               % if divisible by 5
      /p 0 def                                     % clear flag
      (Buzz) print                                     % and print "Buzz"
    } if
    p 1 eq {                                       % if flag is set
      i str cvs print                                  % convert to text and print
    } if                                           
    (\n) print
} for
quit
